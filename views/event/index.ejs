<%- include('../partials/header.ejs')%>
    <main>
      <article class="all-events">
        <h3>All Events</h3>
        <% for(const [category, events] of eventMap) { %>
        <article class="event-category">
          <h4><%=category%></h4>
          <ul class="event-cards">
            <% for (const event of events) { %>
            <li class="card">
              <img
                src= <%= event.eventImg%>
               />
              <a href="/events/<%= event.id%>"
                ><%= event.name%></a
              >
            </li>
            <% } %>
          </ul>
        </article>
        <% } %>
      </article>
    </main>
<%- include('../partials/footer.ejs')%>